    pairwise manner, using the single value if one list runs out
    first.  [3pt]
    # zipwith;;
    - : ('a -> 'a -> 'b) -> 'a -> 'a list -> 'a list -> 'b list =
    <fun>
    # (+);;
    - : int -> int -> int = <fun>
    # zipwith (+) 0;;
    - : int list -> int list -> int list = <fun>
    # zipwith (+) 0 [1; 2; 3] [4; 5; 6; 7; 8];;
    - : int list = [5; 7; 9; 7; 8]
    # zipwith (^) "**" ["foo"] ["bar"; "baz"];;
    - : string list = ["foobar"; "**baz"]
    # zipwith (^) "**" ["foo"; "bar"] ["baz"];;
    - : string list = ["foobaz"; "bar**"]

